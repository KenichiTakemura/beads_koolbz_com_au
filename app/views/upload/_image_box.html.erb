<% if @images.present? %>
<ul class="thumbnails">
	<% @images.each do |image| %>
	<li class="span3 pull-left">
		<div class="thumbnail">
			<%= image_tag(image.medium_image)%>
			<%= link_to(t("op.close"),image_path(image),:class=>"image-delete pull-right",:remote => true, :method => :delete)%>
			<p><%= image.something%></p>
			<p><%= image.filename%></p>
			<p><%= image.filesize%>&nbsp;byte</p>
		</div>
	</li>
	<% end %>
</ul>
<% end %>
